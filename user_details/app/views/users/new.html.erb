<h1>Users#new</h1>
<%= form_for @user do |user| %>
<%= user.label :name%>
<%= user.text_field :name%>

<%= user.label :email%>
<%= user.text_field :email%>

<%= user.label :password%>
<%= user.text_field :password%>

<%= user.label :confirm_password%>
<%= user.text_field :confirm_password%>
<%=user.submit%>
<%end%>