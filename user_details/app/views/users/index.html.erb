<div class = "row index-page">
	<div class ="col-md-8 col-md-offset-1 register_form">
		<table>
			<tr>
				<th width = 200>Name</th>
				<th width = 300>Email</th>
				<th width = 100></th>
				<th width = 200></th>
				<th width = 200></th>
			</tr>
			<%@users.each do |user|%>
				<tr class = "not-header">
					<td align = "left"><%= link_to user.name, user_path(user)%></td>
					<td align = "center"><%= user.email%></td>
					<td align = "center"><%= link_to "Profile", user_path(user)%></td>
					<td align = "center"><%= link_to "Edit", edit_user_path(user), class: "btn btn-success"  %></td>
					<td align = "center"><%= link_to "Delete",user,method: :delete, data: {confirm: "Are you sure?"}%></td>
				</tr>
			<%end%>
			
		</table>
	</div>
</div>